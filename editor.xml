  </component>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="IS_WHITESPACES_SHOWN" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern pattern="\btodo\b.*ghy" />
    <pattern pattern="\btodo\b.*i18n" />
    <pattern pattern="\btodo\b.*Harry" />
    <filter name="mine">
      <pattern index="2" />
      <pattern index="4" />
    </filter>
    <filter name="i18n">
      <pattern index="3" />
    </filter>
  </component>
</application>